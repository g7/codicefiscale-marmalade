CIwUIElement
{
	name ExpMetaPalette:s/AddSpacer

	CIwUIElement
	{
		name			Spacer
		
		CIwUILayout
		{
			element {
				name			Spacer
				CIwUILayoutSpacer {
					name "Spacer"
					sizeMin  { 30 30 }
					sizeMax  { 30 30 }
					sizeHint  { 30 30 }
				}
			}
		}
	}
}

CIwUIElement
{
	name ExpMetaPalette:p/SizerPolicy

	CIwUIElement
	{
		name "SizeToContent"
		sizeMin  { -1 -1 }
		sizeMax  { -1 -1 }
		sizeHint  { -1 -1 }
		sizeToContent true
	}
	CIwUIElement
	{
		name "ProportionalSize"
		sizeMin  { 16 16 }
		sizeMax  { -1 -1 }
		sizeHint  { 500 500 }
		sizeToContent false
	}
	CIwUIElement
	{
		name "FixedSize"
		sizeMin  { 16 16 }
		sizeMax  { 16 16 }
		sizeHint  { 16 16 }
		sizeToContent false
	}
}
