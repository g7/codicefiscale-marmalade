CIwUIElement
{
	name ExpMetaPalette:f/ChangeElementsFocus

	CIwUIElement
	{
		name			NoFocus
	}
	CIwUIElement
	{
		name			FocusHandler2D
		
		CIwUIFocusHandler2D
		{
			name	FocusHandler2D
		}
	}
	CIwUIElement
	{
		name			FocusHandlerTab
		
		CIwUIFocusHandlerTab
		{
			name	FocusHandlerTab
